<html>
<head>
  <%- include('./partial/header'); %>
</head>
<body>
<%- include('./partial/nav'); %>
<hr/>
<div class="container">
    <div class="row">
        <h6 class="card-subtitle mb-2 text-muted">Login User: <%- decodedAccessToken.preferred_username %></h6>
    </div>
    <div class="row">
        <div class="col-6">
            <div class="card" style="width: 100%;">
                <div class="card-body">
                  <h5 class="card-title">Id Token</h5>
                  <p class="card-text">
                    <code>
                        <pre>
<%- JSON.stringify(decodedIdToken, null, 2) %>
                        </pre>
                    </code>
                  </p>
                </div>
            </div>
        </div>
        <div class="col-6">
            <div class="card" style="width: 100%;">
                <div class="card-body">
                  <h5 class="card-title">Access Token</h5>
                  <p class="card-text">
                    <code>
                        <pre>
<%- JSON.stringify(decodedAccessToken, null, 2) %>
                        </pre>
                    </code>
                  </p>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>